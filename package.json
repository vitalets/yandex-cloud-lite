{
  "name": "yandex-cloud-lite",
  "description": "Yandex Cloud SDK lite",
  "version": "1.1.4",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist",
    "generated",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "tsc": "tsc --noEmit",
    "test": "mocha -r ts-node/register test/setup.ts test/specs/*.ts",
    "update-proto": "./scripts/update-proto.sh",
    "compile-proto": "./scripts/compile-proto.sh",
    "check": "ts-node scripts/check.ts",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "release": "npm run lint && npm t && npm run build && npm version $VER && npm publish && git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@types/google-protobuf": "^3.15.5",
    "exponential-backoff": "^3.1.0",
    "google-protobuf": "^3.18.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-mocha": "^9.0.0",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.1",
    "protobufjs": "^6.11.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.2.4"
  },
  "author": {
    "name": "Vitaliy Potapov"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/yandex-cloud-lite.git"
  },
  "license": "MIT"
}
